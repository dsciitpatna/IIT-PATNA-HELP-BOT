  <a href="/home" class="but fa" style="position: fixed; font-size: 45px; text-align: center;  z-index: 50;" id="back"><div id="ch">&#xf060;</div></a><br>
  <h1 style="display: table; margin: auto; padding: 20px">Transport</h1>

  <table cellpadding="0" cellspacing="0" border="0" class="container">
    <div class="tbl-header">
      <thead>
        <tr style="color:white;">
          <th>Bus Number</th><th>Departure Place</th><th>Departure Time</th><th>Destination</th><th>Arrival Time</th><th>Recent Location</th></tr></thead></div>
          <tbody>
            <% timing.forEach(function(row) { %>
            <tr><td><%= row.BusN %></td><td><%= row.DepartedFrom %></td><td><%= row.DepartureTime %></td><td><%= row.ArrivalPlace %></td><td><%= row.ArrivalTime %></td><td><%= row.RecentPlace %></td><td><%= row.ac %></td></tr>
            <% }); %>
          </tbody>
        </table>

        <button onclick="window.location.href='/schedule'" class="btn"><div id="btn">SCHEDULE</div></button>
        <button onclick="window.location.href='/cbooking'" class="btn1"><div id="btn1">Book A Car</div></button>

        <img src="../images/iitp.png"/ id="logo">
      </body>
      <input type="hidden" name="speech-msg" id="speech-msg" x-webkit-speech> 

      <br><br>

      <script type="text/javascript" src="js/converting.js"></script>
      <script type="text/javascript">
        function start() {
          console.log("before start");
          startConverting(null,function(msg) {
            console.log("Started");

            msg = msg.toUpperCase();
            if(msg.indexOf("BACK")>=0 || msg.indexOf("PREVIOUS PAGE")>=0)
              $("#ch").click();
            if(msg.indexOf("DONE")>=0 || msg.indexOf("FINISH")>=0 || msg.indexOf("TERMINATE")>=0 || msg.indexOf("END")>=0)
              window.location.href = '/';
            $("#back").click();
            if(msg.indexOf("SCHEDULE")>=0)
              $("#btn").click()
            if(msg.indexOf("BOOK A CAR")>=0 || msg.indexOf("CAR")>=0)
              $("#btn1").click()
            console.log(msg);
            setTimeout(  function( ){start();},5000 );
            console.log(msg);
          });
        }
        speak("Do you want to view the schedule or book a car");
        setTimeout(  function( ){start();},3000 );
      </script>
